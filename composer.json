{
	"name": "moonwalkingbits/wordpress-configuration-options-configuration-source",
	"description": "WordPress options configuration source.",
	"type": "library",
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Martin Pettersson",
			"email": "martin@moonwalkingbits.com"
		}
	],
	"minimum-stability": "stable",
	"scripts": {
		"test": "phpunit",
		"lint": "phpcs -p --standard=WordPress src/",
		"analyze": [
			"phpmd --suffixes php src/ ansi phpmd.xml",
			"phan --output-mode text --no-progress-bar --color --allow-polyfill-parser"
		]
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"autoload-dev": {
		"classmap": [
			"test/"
		]
	},
	"require": {
		"php": ">=7.4",
		"moonwalkingbits/wordpress-configuration": "^0.1.1"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.6",
		"wp-coding-standards/wpcs": "^2.3",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
		"phpmd/phpmd": "^2.10",
		"phan/phan": "^5.3",
		"phpunit/phpunit": "^9.5",
		"php-mock/php-mock-phpunit": "^2.6",
		"johnpbloch/wordpress-core": "^5.8"
	}
}
